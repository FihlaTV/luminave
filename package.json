{
  "name": "VisionLord",
  "version": "0.0.1",
  "description": "Show manager",
  "main": "index.js",
  "scripts": {
    "test": "mocha --recursive --require babel-register test/",
    "start": "node index.js",
    "build": "babel -q server -d server-bin/ && browserify client/index.js -t babelify --outfile client-bin/js/index.js",
    "watch": "npm run watch-client & npm run watch-server",
    "watch-dev": "npm run watch-client & npm run watch-server",
    "watch-server": "babel-watch server",
    "watch-client": "watchify client/index.js -t babelify --outfile client-bin/js/index.js",
    "compile": "export NODE_ENV=production && npm run build && npm run start",
    "compile-dev": "export NODE_ENV=development && npm run build && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NERDDISCO/VisionLord.git"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.2",
    "express-ws": "^3.0.0",
    "fivetwelve": "^1.0.0-alpha.1",
    "fivetwelve-driver-usbpro": "^1.0.2",
    "jade": "^1.11.0",
    "morgan": "^1.8.1",
    "nodejs-websocket": "^1.7.1",
    "path": "^0.12.7",
    "random-color": "^1.0.1",
    "serialport": "^4.0.7",
    "serve-favicon": "^2.4.2",
    "virtual-serialport": "^0.3.3"
  },
  "author": "Tim Pietrusky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NERDDISCO/VisionLord/issues"
  },
  "homepage": "https://github.com/NERDDISCO/VisionLord#readme",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.2.2",
    "babel-register": "^6.24.0",
    "babel-watch": "^2.0.6",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "mocha": "^3.2.0",
    "watchify": "^3.9.0"
  }
}
